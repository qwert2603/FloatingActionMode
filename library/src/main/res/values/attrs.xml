<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FloatingActionMode">
        <attr name="opened" format="boolean"/>
        <attr name="content_res" format="reference"/>
        <attr name="can_close" format="boolean"/>
        <attr name="close_icon" format="reference"/>
        <attr name="can_drag" format="boolean"/>
        <attr name="drag_icon" format="reference"/>
        <attr name="can_dismiss" format="boolean"/>
        <attr name="dismiss_threshold" format="float"/>
        <attr name="minimize_direction" format="enum">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
            <enum name="nearest" value="2"/>
        </attr>
        <attr name="animation_duration" format="integer"/>
    </declare-styleable>

</resources>